{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && vite build",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "prettier": "prettier --check 'src/**/*.{ts,tsx,js,jsx,json,css,scss,md}'",
    "prettier:fix": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,css,scss,md}'",
    "preview": "vite preview",
    "test": "vitest run --reporter verbose --exclude 'tests/integration/**/*.ts'",
    "test:integration": "vitest --config vitest.integration.config.ts"
  },
  "dependencies": {
    "axios": "^1.13.1",
    "dotenv": "^17.2.3",
    "ms": "^2.1.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.4",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tanstack/react-query": "^5.90.6",
    "@types/ms": "^2.1.0",
    "@types/node": "^24.9.1",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",

    "fetch-cookie": "^3.1.0",
    "globals": "^16.4.0",
    "node-fetch": "^3.3.2",
    "prettier": "^3.7.1",
    "typescript": "~5.8.3",
    "vite": "^7.1.7",
    "vitest": "^4.0.14"
  }
}
